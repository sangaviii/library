<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_books">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h2>Books Report</h2>
                        <p>Name :
                            <span t-field="o.name"/>
                        </p>
                        <span t-field="o.sales_price"/>
                    </div>
                </t>
            </t>
        </t>
    </template>

</odoo>